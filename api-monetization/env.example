# SwaggyStacks API Monetization System Environment Variables

# =============================================================================
# API Configuration
# =============================================================================
PORT=8000
WORKERS=4
HOST=0.0.0.0
DEBUG=false
LOG_LEVEL=INFO

# =============================================================================
# Database Configuration
# =============================================================================
DATABASE_URL=postgresql://swaggystacks:password@localhost:5432/swaggystacks
DATABASE_POOL_SIZE=10
DATABASE_MAX_OVERFLOW=20

# =============================================================================
# Redis Configuration
# =============================================================================
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=
REDIS_DB=0

# =============================================================================
# Stripe Payment Processing
# =============================================================================
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# =============================================================================
# Alpaca Trading API
# =============================================================================
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here
ALPACA_BASE_URL=https://paper-api.alpaca.markets/v2
ALPACA_DATA_URL=https://data.alpaca.markets/v2

# =============================================================================
# Security Configuration
# =============================================================================
JWT_SECRET_KEY=your_jwt_secret_key_here_minimum_32_characters
API_SECRET_KEY=your_api_secret_key_here
ENCRYPTION_KEY=your_encryption_key_here_32_bytes

# =============================================================================
# External API Keys
# =============================================================================
# Yahoo Finance (for market data)
YAHOO_FINANCE_API_KEY=

# Alpha Vantage (alternative data source)
ALPHA_VANTAGE_API_KEY=

# News API (for sentiment analysis)
NEWS_API_KEY=

# =============================================================================
# Monitoring and Analytics
# =============================================================================
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000
SENTRY_DSN=

# =============================================================================
# Email Configuration
# =============================================================================
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_password
SMTP_FROM_EMAIL=noreply@swaggystacks.com

# =============================================================================
# Webhook Configuration
# =============================================================================
WEBHOOK_SECRET=your_webhook_secret_here
WEBHOOK_TIMEOUT=30

# =============================================================================
# Rate Limiting Configuration
# =============================================================================
RATE_LIMIT_ENABLED=true
RATE_LIMIT_STORAGE=redis
RATE_LIMIT_WINDOW=60

# =============================================================================
# CORS Configuration
# =============================================================================
CORS_ORIGINS=["http://localhost:3000", "https://swaggystacks.com"]
CORS_CREDENTIALS=true
CORS_METHODS=["GET", "POST", "PUT", "DELETE", "OPTIONS"]
CORS_HEADERS=["*"]

# =============================================================================
# File Storage
# =============================================================================
# AWS S3 (for file storage)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
AWS_S3_BUCKET=swaggystacks-files

# =============================================================================
# Development Configuration
# =============================================================================
ENVIRONMENT=development
RELOAD=true
LOG_FORMAT=json

# =============================================================================
# Production Configuration
# =============================================================================
# Set these for production deployment
# ENVIRONMENT=production
# DEBUG=false
# RELOAD=false
# LOG_FORMAT=json

# =============================================================================
# Feature Flags
# =============================================================================
ENABLE_MCP_SERVER=true
ENABLE_WEBHOOKS=true
ENABLE_ANALYTICS=true
ENABLE_BACKTESTING=true
ENABLE_LIVE_TRADING=false

# =============================================================================
# Subscription Configuration
# =============================================================================
FREE_TIER_QUOTA=100
BASIC_TIER_QUOTA=1000
PRO_TIER_QUOTA=10000
ENTERPRISE_TIER_QUOTA=100000

# =============================================================================
# MCP Server Configuration
# =============================================================================
MCP_SERVER_PORT=8001
MCP_SERVER_HOST=0.0.0.0
MCP_MAX_CONNECTIONS=100
MCP_TIMEOUT=300

# =============================================================================
# Trading Configuration
# =============================================================================
DEFAULT_ACCOUNT_SIZE=100000
MAX_POSITION_SIZE=0.1
RISK_FREE_RATE=0.02
DEFAULT_LOOKBACK_PERIOD=252

# =============================================================================
# Notification Configuration
# =============================================================================
SLACK_WEBHOOK_URL=
DISCORD_WEBHOOK_URL=
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
