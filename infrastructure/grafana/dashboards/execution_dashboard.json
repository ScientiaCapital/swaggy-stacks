{
  "id": null,
  "uid": "trading-execution",
  "title": "Trade Execution Dashboard",
  "tags": ["trading", "execution", "performance", "latency", "swaggy-stacks"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 12,
      "title": "Trade Execution Success Rate",
      "type": "gauge",
      "targets": [
        {
          "expr": "trading_execution_success_rate",
          "refId": "A",
          "legendFormat": "{{symbol}} - {{order_type}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.8
              },
              {
                "color": "green",
                "value": 0.95
              }
            ]
          },
          "unit": "percentunit",
          "min": 0,
          "max": 1
        },
        "overrides": []
      }
    },
    {
      "id": 13,
      "title": "Trade Execution Failures Analysis",
      "type": "piechart",
      "targets": [
        {
          "expr": "sum by (failure_reason) (trading_execution_failures_total)",
          "refId": "A",
          "legendFormat": "{{failure_reason}}"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "pieType": "pie",
        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "legend": {
          "displayMode": "visible",
          "placement": "bottom"
        },
        "displayLabels": [
          "name",
          "percent"
        ]
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            }
          },
          "mappings": [],
          "unit": "short"
        },
        "overrides": []
      }
    },
    {
      "id": 14,
      "title": "Trade Execution Latency Distribution",
      "type": "histogram",
      "targets": [
        {
          "expr": "trading_execution_latency_seconds",
          "refId": "A",
          "legendFormat": "{{operation_type}} - {{symbol}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 12,
        "h": 8
      },
      "options": {
        "legend": {
          "displayMode": "visible",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.1
              },
              {
                "color": "red",
                "value": 1.0
              }
            ]
          },
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "id": 15,
      "title": "Execution Success vs Failures Timeline",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(trading_execution_total{status=\"filled\"}[5m])",
          "refId": "A",
          "legendFormat": "Successful Executions/sec"
        },
        {
          "expr": "rate(trading_execution_failures_total[5m])",
          "refId": "B",
          "legendFormat": "Failed Executions/sec"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 8,
        "w": 12,
        "h": 8
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "visible",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 30,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 10
              }
            ]
          },
          "unit": "reqps"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Successful Executions/sec"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Failed Executions/sec"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 16,
      "title": "Execution Latency by Operation Type",
      "type": "bargauge",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, trading_execution_latency_seconds)",
          "refId": "A",
          "legendFormat": "{{operation_type}} P95"
        },
        {
          "expr": "histogram_quantile(0.50, trading_execution_latency_seconds)",
          "refId": "B",
          "legendFormat": "{{operation_type}} P50"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 12,
        "h": 8
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "minVizWidth": 75,
        "minVizHeight": 75,
        "showUnfilled": true
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "continuous-GrYlRd"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.05
              },
              {
                "color": "red",
                "value": 0.2
              }
            ]
          },
          "unit": "s",
          "min": 0,
          "max": 1
        },
        "overrides": []
      }
    },
    {
      "id": 17,
      "title": "Trade Execution Details by Symbol",
      "type": "table",
      "targets": [
        {
          "expr": "trading_execution_success_rate",
          "refId": "A",
          "legendFormat": "Success Rate",
          "format": "table"
        },
        {
          "expr": "sum by (symbol) (trading_execution_total)",
          "refId": "B",
          "legendFormat": "Total Executions",
          "format": "table"
        },
        {
          "expr": "sum by (symbol) (trading_execution_failures_total)",
          "refId": "C",
          "legendFormat": "Total Failures",
          "format": "table"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 16,
        "w": 12,
        "h": 8
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "Success Rate"
          }
        ]
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.8
              },
              {
                "color": "green",
                "value": 0.95
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Success Rate"
            },
            "properties": [
              {
                "id": "unit",
                "value": "percentunit"
              },
              {
                "id": "custom.displayMode",
                "value": "color-background"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Total Executions"
            },
            "properties": [
              {
                "id": "unit",
                "value": "short"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Total Failures"
            },
            "properties": [
              {
                "id": "unit",
                "value": "short"
              },
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      }
    }
  ],
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h",
      "2h",
      "1d"
    ]
  },
  "templating": {
    "list": []
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "links": [
    {
      "title": "P&L Dashboard",
      "url": "/d/trading-pnl/trading-pnl-dashboard"
    },
    {
      "title": "Strategy Performance",
      "url": "/d/trading-strategy/strategy-performance-dashboard"
    },
    {
      "title": "Risk Monitoring",
      "url": "/d/trading-risk/risk-monitoring-dashboard"
    }
  ],
  "templating": {
    "list": [
      {
        "allValue": ".*",
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        },
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "definition": "label_values(trading_pnl_total_usd, symbol)",
        "hide": 0,
        "includeAll": true,
        "label": "Symbol",
        "multi": true,
        "name": "symbol",
        "options": [],
        "query": {
          "query": "label_values(trading_pnl_total_usd, symbol)",
          "refId": "StandardVariableQuery"
        },
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "type": "query"
      },
      {
        "allValue": ".*",
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        },
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "definition": "label_values(trading_execution_latency_seconds, order_type)",
        "hide": 0,
        "includeAll": true,
        "label": "Order Type",
        "multi": true,
        "name": "order_type",
        "options": [],
        "query": {
          "query": "label_values(trading_execution_latency_seconds, order_type)",
          "refId": "StandardVariableQuery"
        },
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "type": "query"
      }
    ]
  },
  "refresh": "5s",
  "schemaVersion": 30,
  "version": 2,
  "weekStart": "",
  "description": "Trade execution analytics dashboard showing success rates, failure analysis, latency metrics, and operational performance monitoring for SwaggyStacks trading system. Includes dynamic filtering by symbol and order type."
}