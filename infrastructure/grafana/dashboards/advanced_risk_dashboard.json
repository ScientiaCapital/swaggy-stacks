    {
      "id": 8,
      "title": "Correlation Heatmap",
      "type": "heatmap",
      "targets": [
        {
          "expr": "trading_asset_correlation{symbol_a=~\"$symbol\", symbol_b=~\"$symbol\"}",
          "refId": "A",
          "legendFormat": "{{symbol_a}} vs {{symbol_b}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 24,
        "w": 12,
        "h": 8
      },
      "options": {
        "calculate": false,
        "cellGap": 2,
        "cellValues": {},
        "color": {
          "exponent": 0.5,
          "fill": "dark-orange",
          "mode": "spectrum",
          "reverse": false,
          "scale": "exponential",
          "scheme": "RdYlGn",
          "steps": 64
        },
        "exemplars": {
          "color": "rgba(255,0,255,0.7)"
        },
        "filterValues": {
          "le": 1e-9
        },
        "legend": {
          "show": true
        },
        "rowsFrame": {
          "layout": "auto"
        },
        "showValue": "never",
        "tooltip": {
          "show": true,
          "yHistogram": false
        },
        "yAxis": {
          "axisPlacement": "left",
          "reverse": false,
          "unit": "short"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": -1,
          "max": 1
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
