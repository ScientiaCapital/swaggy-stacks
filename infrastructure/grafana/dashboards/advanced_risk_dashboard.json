{
  "id": null,
  "uid": "advanced-risk",
  "title": "Advanced Risk Dashboard",
  "tags": ["risk", "exposure", "portfolio", "management", "swaggy-stacks"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Portfolio Value at Risk (VaR)",
      "type": "stat",
      "targets": [
        {
          "expr": "trading_portfolio_var_95",
          "refId": "A",
          "legendFormat": "95% VaR"
        },
        {
          "expr": "trading_portfolio_var_99",
          "refId": "B",
          "legendFormat": "99% VaR"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": -5000
              },
              {
                "color": "red",
                "value": -10000
              }
            ]
          },
          "unit": "currencyUSD"
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
    {
      "id": 2,
      "title": "Risk Limit Utilization",
      "type": "gauge",
      "targets": [
        {
          "expr": "(trading_portfolio_exposure_total / trading_max_portfolio_exposure) * 100",
          "refId": "A",
          "legendFormat": "Portfolio Exposure %"
        },
        {
          "expr": "(trading_daily_loss_current / trading_daily_loss_limit) * 100",
          "refId": "B",
          "legendFormat": "Daily Loss Limit %"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 70
              },
              {
                "color": "red",
                "value": 90
              }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
    {
      "id": 3,
      "title": "Portfolio Exposure by Asset Class",
      "type": "piechart",
      "targets": [
        {
          "expr": "trading_portfolio_exposure_by_asset_class{asset_class=~\"$asset_class\"}",
          "refId": "A",
          "legendFormat": "{{asset_class}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 8,
        "h": 8
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "pieType": "pie",
        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "legend": {
          "displayMode": "visible",
          "placement": "right"
        },
        "displayLabels": ["name", "percent"]
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "currencyUSD"
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
    {
      "id": 4,
      "title": "Portfolio Exposure by Sector",
      "type": "barchart",
      "targets": [
        {
          "expr": "trading_portfolio_exposure_by_sector{sector=~\"$sector\"}",
          "refId": "A",
          "legendFormat": "{{sector}}"
