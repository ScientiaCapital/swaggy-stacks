          "mappings": [
            {
              "options": {
                "0": {
                  "text": "OK",
                  "color": "green"
                },
                "1": {
                  "text": "WARNING",
                  "color": "yellow"
                },
                "2": {
                  "text": "CRITICAL",
                  "color": "red"
                }
              },
              "type": "value"
            }
          ]
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
