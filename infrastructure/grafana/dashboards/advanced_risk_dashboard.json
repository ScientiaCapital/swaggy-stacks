      "options": {
        "orientation": "horizontal",
        "barWidth": 0.97,
        "groupWidth": 0.7,
        "barRadius": 0,
        "showValue": "auto",
        "stacking": "none",
        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "text": {}
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "currencyUSD"
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
