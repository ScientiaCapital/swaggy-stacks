        {
          "expr": "trading_sharpe_ratio{symbol=~\"$symbol\"}",
          "refId": "A",
          "legendFormat": "Sharpe Ratio {{symbol}}"
        },
        {
          "expr": "trading_sortino_ratio{symbol=~\"$symbol\"}",
          "refId": "B",
          "legendFormat": "Sortino Ratio {{symbol}}"
