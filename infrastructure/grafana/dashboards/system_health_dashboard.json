          "text": "All",
          "value": "$__all"
        },
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "definition": "label_values(trading_component_health_status, component)",
        "hide": 0,
        "includeAll": true,
        "label": "Component",
        "multi": true,
        "name": "component",
        "options": [],
        "query": {
          "query": "label_values(trading_component_health_status, component)",
          "refId": "StandardVariableQuery"
        },
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "type": "query"
      },
      {
        "allValue": ".*",
