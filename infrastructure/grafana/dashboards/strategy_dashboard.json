{
  "id": null,
  "uid": "trading-strategy",
  "title": "Strategy Performance Dashboard",
  "tags": ["trading", "strategy", "performance", "swaggy-stacks"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 7,
      "title": "Strategy Win/Loss Ratios",
      "type": "bargauge",
      "targets": [
        {
          "expr": "trading_strategy_win_loss_ratio",
          "refId": "A",
          "legendFormat": "{{strategy_name}} - {{symbol}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "basic",
        "minVizWidth": 75,
        "minVizHeight": 75,
        "showUnfilled": true
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "continuous-GrYlRd"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "green",
                "value": 2
              }
            ]
          },
          "unit": "short",
          "min": 0,
          "max": 5
        },
        "overrides": []
      }
    },
    {
      "id": 8,
      "title": "Strategy Success Rates",
      "type": "table",
      "targets": [
        {
          "expr": "trading_strategy_success_rate",
          "refId": "A",
          "legendFormat": "{{strategy_name}} - {{symbol}}",
          "format": "table"
        },
        {
          "expr": "trading_strategy_total_trades{outcome=\"win\"}",
          "refId": "B",
          "legendFormat": "{{strategy_name}} - {{symbol}} Wins",
          "format": "table"
        },
        {
          "expr": "trading_strategy_total_trades{outcome=\"loss\"}",
          "refId": "C",
          "legendFormat": "{{strategy_name}} - {{symbol}} Losses",
          "format": "table"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "Success Rate"
          }
        ]
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.5
              },
              {
                "color": "green",
                "value": 0.7
              }
            ]
          },
          "unit": "percentunit"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "strategy_name"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 120
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "symbol"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 80
              }
            ]
          }
        ]
      }
    },
    {
      "id": 9,
      "title": "Strategy Drawdown Analysis",
      "type": "timeseries",
      "targets": [
        {
          "expr": "trading_strategy_drawdown_current_pct",
          "refId": "A",
          "legendFormat": "{{strategy_name}} Current DD"
        },
        {
          "expr": "trading_strategy_drawdown_max_pct",
          "refId": "B",
          "legendFormat": "{{strategy_name}} Max DD"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 12,
        "h": 8
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "visible",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 20,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "thresholdsStyle": {
              "mode": "line"
            }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": -10
              },
              {
                "color": "red",
                "value": -20
              }
            ]
          },
          "unit": "percent",
          "max": 0,
          "min": -50
        },
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": ".*Max DD.*"
            },
            "properties": [
              {
                "id": "custom.drawStyle",
                "value": "bars"
              },
              {
                "id": "custom.fillOpacity",
                "value": 50
              }
            ]
          }
        ]
      }
    },
    {
      "id": 10,
      "title": "Strategy Performance Matrix",
      "type": "heatmap",
      "targets": [
        {
          "expr": "trading_strategy_avg_profit_loss_usd{trade_type=\"profit\"}",
          "refId": "A",
          "legendFormat": "{{strategy_name}} - {{symbol}}"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 8,
        "w": 12,
        "h": 8
      },
      "options": {
        "calculate": false,
        "cellGap": 2,
        "cellValues": {},
        "color": {
          "exponent": 0.5,
          "fill": "dark-orange",
          "mode": "spectrum",
          "reverse": false,
          "scale": "exponential",
          "scheme": "Spectral",
          "steps": 64
        },
        "exemplars": {
          "color": "rgba(255,0,255,0.7)"
        },
        "filterValues": {
          "le": 1e-9
        },
        "legend": {
          "show": false
        },
        "rowsFrame": {
          "layout": "auto"
        },
        "tooltip": {
          "show": true,
          "yHistogram": false
        },
        "yAxis": {
          "axisPlacement": "left",
          "reverse": false,
          "unit": "short"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "scaleDistribution": {
              "type": "linear"
            }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          },
          "unit": "currencyUSD"
        },
        "overrides": []
      }
    },
    {
      "id": 11,
      "title": "Strategy Average P&L Comparison",
      "type": "barchart",
      "targets": [
        {
          "expr": "trading_strategy_avg_profit_loss_usd{trade_type=\"profit\"}",
          "refId": "A",
          "legendFormat": "{{strategy_name}} Avg Profit"
        },
        {
          "expr": "abs(trading_strategy_avg_profit_loss_usd{trade_type=\"loss\"})",
          "refId": "B",
          "legendFormat": "{{strategy_name}} Avg Loss"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 24,
        "h": 8
      },
      "options": {
        "orientation": "auto",
        "barWidth": 0.97,
        "groupWidth": 0.7,
        "legend": {
          "displayMode": "visible",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisPlacement": "auto",
            "axisLabel": "",
            "axisWidth": 80,
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 80,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 1000
              }
            ]
          },
          "unit": "currencyUSD"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": ".*Profit.*"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": ".*Loss.*"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      }
    }
  ],
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h",
      "2h",
      "1d"
    ]
  },
  "templating": {
    "list": []
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "links": [
    {
      "title": "P&L Dashboard",
      "url": "/d/trading-pnl/trading-pnl-dashboard"
    },
    {
      "title": "Trade Execution",
      "url": "/d/trading-execution/trade-execution-dashboard"
    },
    {
      "title": "Risk Monitoring",
      "url": "/d/trading-risk/risk-monitoring-dashboard"
    }
  ],
  "templating": {
    "list": [
      {
        "allValue": ".*",
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        },
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "definition": "label_values(trading_strategy_success_rate, strategy_name)",
        "hide": 0,
        "includeAll": true,
        "label": "Strategy",
        "multi": true,
        "name": "strategy",
        "options": [],
        "query": {
          "query": "label_values(trading_strategy_success_rate, strategy_name)",
          "refId": "StandardVariableQuery"
        },
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "type": "query"
      },
      {
        "allValue": ".*",
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        },
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "definition": "label_values(trading_pnl_total_usd, symbol)",
        "hide": 0,
        "includeAll": true,
        "label": "Symbol",
        "multi": true,
        "name": "symbol",
        "options": [],
        "query": {
          "query": "label_values(trading_pnl_total_usd, symbol)",
          "refId": "StandardVariableQuery"
        },
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "type": "query"
      }
    ]
  },
  "refresh": "5s",
  "schemaVersion": 30,
  "version": 2,
  "weekStart": "",
  "description": "Strategy performance comparison dashboard showing win/loss ratios, success rates, drawdown analysis, and performance matrices for SwaggyStacks trading strategies. Includes dynamic filtering by strategy and symbol."
}